{
    "manifest_version": 3,
    "name": "E-commerce Helper Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "exclude_matches": ["*://*.google.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }